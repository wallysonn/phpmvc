{
    "name": "wds/phpmvc",
    "description": "Simple PHPFramework in MVC",
    "keywords": ["mvc", "php", "phpmvc", "framework"],
    "license": "MIT",
    "type": "phpmvc",
    "authors": [{
        "name": "Walison Gomes",
        "email": "wgwalisongomes@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "WDS\\PhpMVC\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WDS\\PhpMVC\\": "test/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
